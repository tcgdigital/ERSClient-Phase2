  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "lint": "npm run tslint \"src/**/*.ts\" --force",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.config.js",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.config.js  --progress --profile",
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && webpack --config config/webpack.prod.config.js  --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "clean": "npm cache clean && npm run clean:dist && npm run clean:dll && npm run clean:aot && npm run clean:doc && npm run rimraf -- node_modules",
    "clean:dist": "npm run rimraf -- dist",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:doc": "npm run rimraf -- doc",
    "clean:install": "npm set progress=false && npm install",
    "server:dev": "webpack-dev-server --config config/webpack.dev.config.js --colors --port 3000 --progress --profile --watch --content-base src/",
    "server:dashboard": "webpack-dashboard -- webpack-dev-server --config config/webpack.dev.config.js --colors --port 3000 --progress --profile --watch --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 4000 -c-1 --cors",
    "server": "npm run server:dev",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack --progress --profile --bail"
  },
